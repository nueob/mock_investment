function ch(){
    Highcharts.stockChart('container', {
    chart: {
        events: {
        load: function () {

            // set up the updating of the chart each second
            var series = this.series[0];
            setInterval(function () {
            var x = (new Date()).getTime(), // current time
                y = Math.round(Math.random() * 100);
            series.addPoint([x, y], true, true);
            }, 1000);
        }
        }
    },

    time: {
        useUTC: false
    },

    rangeSelector: {
        buttons: [{
        count: 1,
        type: 'minute',
        text: '1M'
        }, {
        count: 5,
        type: 'minute',
        text: '5M'
        }, {
        type: 'all',
        text: 'All'
        }],
        inputEnabled: false,
        selected: 0
    },

    title: {
        text: 'Live random data'
    },

    exporting: {
        enabled: false
    },

    series: [{
        name: 'Random data',
        data: (function () {
        // generate an array of random data
        var data = [],
            time = (new Date()).getTime(),
            i;

        for (i = -999; i <= 0; i += 1) {
            data.push([
            time + i * 1000,
            Math.round(Math.random() * 100)
            ]);
        }
        return data;
        }())
    }]
    });
}ch();

// 검색 내용 검사
function keywordConfirm() {
  console.log('zz');
  var keyword = $('#searching').val();
  console.log(keyword);

  $.ajax({
      url: "/search/keyword",
      type:"POST",
      data: {
        keyword : keyword
      },
      success:function(result){
          if(result == 100) { // 검색 결과가 있을 경우
            var form = document.createElement('form'); // 폼객체 생성
            var objs;
            objs = document.createElement('input');
            objs.setAttribute('type', 'text'); // 값이 들어있는 녀석의 type
            objs.setAttribute('name', 'searching'); // 객체이름
            objs.setAttribute('value', keyword); //객체값
            form.appendChild(objs);
            form.setAttribute('method', 'post'); //get,post 가능
            form.setAttribute('action', "/search"); //보내는 url
            document.body.appendChild(form);
            form.submit();
          } else if(result == 200) { // 검색 결과가 없을 경우
            alert('검색 결과가 없습니다.');
          } else { // 검색 결과가 여러개 일 경우
            alert('검색 종목을 정확히 입력하여 주십시오');
          }
      }
  })
}
